{
  "description": "Tab Count",
  "manifest_version": 2,
  "name": "tab-count-button",
  "version": "1.3",

  "background": {
    "scripts": ["background.js"]
  },
 
  "browser_action": {
    "default_title": "Open tabs",
    "default_icon": {
      "16": "icons/page-16.png",
      "32": "icons/page-32.png"
    }
  },

  "permissions": [
    "storage"
  ],

  "displayKey": "tabsLastCount"
}
