{
  "description": "Tab Count",
  "manifest_version": 2,
  "name": "Tab Count",
  "version": "1.5",

  "background": {
    "scripts": ["background.js"]
  },
 
  "browser_action": {
    "default_title": "Open tabs",
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "96": "icon.png"
    }
  },

  "permissions": [
    "storage"
  ],

  "displayKey": "tabsLastCount"
}
