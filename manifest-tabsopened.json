{
  "description": "Tabs Opened",
  "manifest_version": 2,
  "name": "tabs-opened-button",
  "version": "1.2",

  "background": {
    "scripts": ["background.js"]
  },
 
  "browser_action": {
    "default_title": "Tabs opened today",
    "default_icon": {
      "16": "icons/page-16.png",
      "32": "icons/page-32.png"
    }
  },

  "permissions": [
    "storage"
  ]
}
