{
  "description": "Tabs Closed",
  "manifest_version": 2,
  "name": "tabs-closed-button",
  "version": "1.6",

  "background": {
    "scripts": ["background.js"]
  },
 
  "browser_action": {
    "default_title": "Tabs closed today",
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "96": "icon.png"
    }
  },

  "permissions": [
    "storage"
  ],

  "displayKey": "tabsClosed"
}
